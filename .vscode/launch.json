{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CUDA C++: Debug rckangaroo",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${workspaceFolder}/rckangaroo",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "preLaunchTask": "make",
      "miDebuggerPath": "/usr/local/cuda/bin/cuda-gdb"
    },
    {
      "name": "CUDA C++: Attach",
      "type": "cuda-gdb",
      "request": "attach",
      "program": "${workspaceFolder}/rckangaroo",
      "processId": "${command:pickProcess}",
      "miDebuggerPath": "/usr/local/cuda/bin/cuda-gdb"
    }
  ]
}
